{
  "rules": {
    ".read": false,
    ".write": false,
    "admins": {
      ".read": "root.child('admins').hasChild(auth.uid)",
    },
    "CurrentVotes": {
      ".read": true,
      "Events": {
        ".write": "root.child('admins').hasChild(auth.uid)",
        "$event": {
          "$uid": {
            ".write": "auth.uid == $uid",
          },
        },
      },
      "Times": {
        ".write": "root.child('admins').hasChild(auth.uid)",
        "$event": {
          "$uid": {
            ".write": "auth.uid == $uid",
          },
        },
      },
    },
    "SingleEvents": {
      ".read": true,
      ".write": "auth.uid == root.child('owner').val()",
    },
    "YearlyEvents": {
      ".read": true,
      ".write": "auth.uid == root.child('owner').val()",
    },
    "MonthlyEvents": {
      ".read": true,
      ".write": "auth.uid == root.child('owner').val()",
    },
    "WeeklyEvents": {
      ".read": true,
      ".write": "auth.uid == root.child('owner').val()",
    },
    "Events": {
      ".read": true,
      ".write": "auth.uid == root.child('owner').val()",
    },
  }
}